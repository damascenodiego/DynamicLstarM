    (not (and G B B B ))
    (not (and G B B S ))
))
(check-sat)
(pop)
(push)
(assert N)
(assert (and 
    (not (and G N ))
))
(check-sat)
(pop)
(push)
(assert W)
(assert (and 
    (not (and G W ))
    (not (and G W P W ))
))
(check-sat)
(pop)
(push)
(assert B)
(assert (and 
    (not (and G B ))
))
(check-sat)
(pop)
(push)
(assert S)
(assert (and 
    (not (and G W W S ))
    (not (and G B B B P B ))
    (not (and G B B S P B ))
    (not (and G N N N P N ))
    (not (and G N N S P N ))
